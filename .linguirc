{
  "locales": [
    "da",
    "en"
  ],
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ],
      "exclude": [
        "src/locales/**"
      ]
    }
  ],
  "format": "po",
  "sourceLocale": "en",
  "fallbackLocales": {
    "default": "en"
  }
}
